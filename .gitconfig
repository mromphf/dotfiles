[user]
	email = markromphf@gmail.com
	name = Mark Romphf
[core]
	autocrlf = true
	editor = vim
[color]
	ui = true
[alias]
	aa = add -A
	b = branch
	ci = commit
	co = checkout
	dc = diff --cached
	di = diff
	lg = log --branches --all --graph --decorate --format=format:\"%C(yellow)%h%C(auto)%d %Creset%s %C(cyan)(%ar) %Cgreen<%an>\" -n 15
	rb = rebase --verbose
	sh = show
	st = status -sb
    ls = diff-tree --no-commit-id --name-only -r
    t = tag
